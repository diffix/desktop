{
  "name": "publisher",
  "version": "0.0.1",
  "private": true,
  "description": "Desktop app for anonymizing data using Open Diffix Publish",
  "main": "./.webpack/main",
  "scripts": {
    "dev:forge": "electron-forge start",
    "dev:fable": "dotnet fable watch OpenDiffix.Publisher -o src/compiled",
    "dev": "run-p dev:*",
    "package": "electron-forge package",
    "make:fable": "dotnet fable OpenDiffix.Publisher -o src/compiled",
    "make:forge": "electron-forge make",
    "make": "run-s make:fable make:forge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diffix/publisher.git"
  },
  "author": "Open Diffix",
  "bugs": {
    "url": "https://github.com/diffix/publisher/issues"
  },
  "homepage": "https://github.com/diffix/publisher#readme",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.57",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "electron": "13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.3",
    "webpack": "^5.38.1"
  }
}